{
  "name": "front-basic",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:DanielFerrariR/front-basic.git",
  "author": "Daniel Ferrari <danielferrari@live.com>",
  "license": "MIT",
  "scripts": {
    "commit": "git add . && git-cz",
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "start": "serve --no-clipboard --single --listen 3000 dist",
    "build": "webpack --config webpack.config.prod.js",
    "prettier": "prettier \"**/*.+(js|jsx|json|ts|tsx|md|mdx|html|eslintrc|prettierrc)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "check-types": "yarn tsc",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.md,.mdx .",
    "validate": "npm-run-all --parallel check-types check-format lint",
    "setup": "yarn && yarn validate",
    "storybook": "start-storybook -p 3001 -c storybook --docs"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
